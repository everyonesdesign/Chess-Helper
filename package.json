{
  "name": "chess-com-helper",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "webpack",
    "watch": "webpack -w",
    "build-prod": "npm run build && uglifyjs app/build.js --compress --mangle -o app/build.js",
    "test": "ts-mocha -p tsconfig.json  app/test/**.ts",
    "e2e": "cypress run --browser chrome --record --headed",
    "e2e-dev": "cypress open",
    "pack": "rm app.zip; npm run build-prod; cd app && zip -r ../app.zip *",
    "release": "node ./release.js"
  },
  "devDependencies": {
    "@types/assert": "^1.5.5",
    "@types/jsdom-global": "^3.0.2",
    "@types/lodash": "^4.14.172",
    "@types/mocha": "^7.0.2",
    "clean-terminal-webpack-plugin": "^3.0.0",
    "cypress": "^8.2.0",
    "jsdom": "^16.7.0",
    "jsdom-global": "3.0.2",
    "mocha": "^9.1.2",
    "ts-loader": "^9.2.5",
    "ts-mocha": "^9.0.2",
    "typescript": "^4.3.5",
    "uglify-es": "^3.3.9",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.0"
  },
  "dependencies": {
    "domify": "^1.4.1",
    "lodash": "^4.17.21",
    "svg.js": "^2.6.5"
  }
}
